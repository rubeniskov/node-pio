  "description": "A simple polls manager using socket.io as connection protocol",
  "main": "app.js",
  "scripts": {
    "app": "node app.js",
    "app:install": "npm install -g bower grunt && bower install --allow-root && npm install",
    "app:build": "grunt build --verbose",
    "app:test": "grunt test --verbose",
    "app:ci-test": "grunt ci-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rubeniskov/node-pio.git"
  },
  "keywords": [
