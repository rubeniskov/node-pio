    "html5",
    "express",
    "socket"
  ],
  "author": "Rubén López Gómez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rubeniskov/node-pio/issues"
  },
  "homepage": "https://github.com/rubeniskov/node-pio",
  "dependencies": {
