    "colors": "^1.1.2",
    "deep-extend": "^0.4.1",
    "express": "^4.13.4",
    "jsonwebtoken": "^5.7.0",
    "mongoose": "^4.4.11",
    "morgan": "^1.7.0",
    "nomnom": "^1.8.1",
    "npm": "^3.8.5",
    "q": "^1.4.1",
    "socket.io": "^1.4.5",
    "twig": "^0.8.9",
    "underscore": "^1.8.3"
