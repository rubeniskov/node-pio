{

    "node": true,
    "browser": true,
    "esnext": true,
    "bitwise": false,
    "camelcase": false,
    "curly": false,
    "eqeqeq": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "quotmark": "single",
    "regexp": true,
    "undef": true,
    "unused": false,
    "strict": false,
    "trailing": true,
    "smarttabs": false,
    "debug": false,
    "boss": true,
    "eqnull": true,
    "expr": true,
    "loopfunc": true,
    "evil": true,

    "globals": {
        "angular": true,
        "moment": true,
        "$": true
    },

    "predef": [
        "define",
        "require",
        "exports",
        "module",
        "describe",
        "before",
        "beforeEach",
        "after",
        "afterEach",
        "it"
    ],

    "indent": 4,
    "maxlen": 200,
    "devel": false,
    "noempty": true
}
